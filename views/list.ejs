<%-include('header.ejs')%>

     <div class="box" id="heading">
          <h1><%=today%></h1>
     </div>
     
     <div class="box">
          
               <% newItems.forEach(newItem => { %>
                    <div class="item">
                         <input type="checkbox" name="" id="">
                         <p><%=newItem%></p>
                    </div>
               <% }); %>

          <form class="item" action="/" method="post">
               <input type="text" name="text" placeholder="New Item" autocomplete="off">
               <button type="submit" name="button">+</button>
          </form>
     </div>
      

<%-include('footer.ejs')%>
